<%@taglib prefix="foo" uri="http://pocs.trongtt.com/t3/jsp/taglib/boy_1_0"%>
<%@taglib prefix="bar" uri="http://pocs.trongtt.com/t3/jsp/taglib/girl_1_0"%>
<html>
<head>
    <title>JSP Custom Taglib example: Substr function</title>
</head>
<body>
    <font color="blue">
    <foo:boy name="Trong" nickname="cuu"/>
    </font>
    LOVE
    <font color="red">
    <bar:girl name="Jolie" age="21"/>
    </font>
</body>
</html>